{
  "name": "ng2-admin",
  "version": "1.0.0",
  "description": "Angular and Bootstrap 4 Admin Template.",
  "author": "Akveo <contact@akveo.com>",
  "homepage": "http://akveo.github.io/ng2-admin/",
  "repository": {
    "type": "git",
    "url": "https://github.com/akveo/ng2-admin.git"
  },
  "bugs": {
    "url": "https://github.com/akveo/ng2-admin/issues"
  },
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "rimraf": "rimraf",
    "changelog": "standard-changelog",
    "start": "ng serve --proxy-config proxy.config.json",
    "start:hmr": "ng serve --hmr",
    "start:aot": "ng serve --aot",
    "start:prod": "ng serve --prod",
    "start:prod:aot": "ng serve --prod --aot",
    "build": "npm run clean:dist && ng build",
    "build:prod": "npm run build -- --prod",
    "build:prod:aot": "npm run build:prod -- --aot",
    "build:ci": "npm run build:prod && npm run build:prod:aot",
    "build:demo": "npm run build:prod:aot -- --base-href \"http://akveo.com/ng2-admin/\"",
    "test": "ng test -sr",
    "test:coverage": "npm run clean:coverage && ng test -sr -cc",
    "lint": "ng lint",
    "lint:styles": "stylelint ./src/app/**/*.scss",
    "lint:ci": "npm run lint && npm run lint:styles",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e",
    "clean:dist": "npm run rimraf -- dist",
    "clean:coverage": "npm run rimraf -- coverage",
    "docs:deploy": "wintersmith build -C docs && gh-pages -d docs/build",
    "docs:serve": "wintersmith preview -C docs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.2.5",
    "@angular/common": "^4.2.5",
    "@angular/compiler": "^4.2.5",
    "@angular/core": "^4.2.5",
    "@angular/forms": "^4.2.5",
    "@angular/http": "^4.2.5",
    "@angular/platform-browser": "^4.2.5",
    "@angular/platform-browser-dynamic": "^4.2.5",
    "@angular/platform-server": "^4.2.5",
    "@angular/router": "^4.2.5",
    "@ng-bootstrap/ng-bootstrap": "1.0.0-alpha.25",
    "@ngrx/core": "^1.2.0",
    "@ngrx/db": "^2.0.2",
    "@ngrx/effects": "^2.0.3",
    "@ngrx/router-store": "^1.2.6",
    "@ngrx/store": "^2.2.3",
    "@ngrx/store-devtools": "^3.2.0",
    "@ngx-translate/core": "6.0.1",
    "@ngx-translate/http-loader": "0.0.3",
    "angular2-datatable": "0.6.0",
    "animate.css": "3.5.2",
    "bootstrap": "4.0.0-alpha.6",
    "chart.js": "1.1.1",
    "chartist": "0.10.1",
    "chroma-js": "^1.3.4",
    "ckeditor": "^4.7.1",
    "core-js": "2.4.1",
    "easy-pie-chart": "2.1.7",
    "font-awesome": "4.7.0",
    "fullcalendar": "^3.4.0",
    "google-maps": "3.2.1",
    "ionicons": "2.0.1",
    "is-empty": "^1.2.0",
    "jquery": "3.2.1",
    "jquery-slimscroll": "1.3.8",
    "leaflet": "0.7.7",
    "leaflet-map": "0.2.1",
    "lodash": "4.17.4",
    "ng2-ckeditor": "^1.1.9",
    "ng2-completer": "^1.5.2",
    "ng2-handsontable": "0.48.0",
    "ng2-slim-loading-bar": "4.0.0",
    "ng2-smart-table": "^1.2.1",
    "ng2-tree": "2.0.0-alpha.5",
    "ngrx-store-freeze": "^0.1.6",
    "ngrx-store-localstorage": "^0.1.8",
    "ngx-uploader": "2.2.5",
    "normalize.css": "6.0.0",
    "reselect": "^3.0.0",
    "roboto-fontface": "0.7.0",
    "rxjs": "^5.4.2",
    "zone.js": "0.8.5"
  },
  "devDependencies": {
    "@angular/cli": "^1.2.0",
    "@angular/compiler-cli": "^4.2.5",
    "@types/fullcalendar": "^2.7.44",
    "@types/jasmine": "^2.5.53",
    "@types/jquery": "2.0.41",
    "@types/jquery.slimscroll": "^1.3.31",
    "@types/lodash": "^4.14.68",
    "@types/node": "6.0.69",
    "angular2-jwt": "^0.2.3",
    "codelyzer": "2.0.0",
    "gh-pages": "0.12.0",
    "jasmine-core": "^2.6.4",
    "jasmine-spec-reporter": "3.2.0",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "0.2.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "npm-run-all": "4.0.2",
    "protractor": "^5.1.2",
    "rimraf": "2.6.1",
    "standard-changelog": "1.0.1",
    "stylelint": "^7.12.0",
    "ts-node": "2.0.0",
    "tslint": "4.5.0",
    "tslint-eslint-rules": "3.5.1",
    "tslint-language-service": "0.9.2",
    "typescript": "^2.4.1",
    "typogr": "0.6.6",
    "underscore": "1.8.3",
    "wintersmith": "^2.4.1",
    "wintersmith-sassy": "1.1.0"
  }
}
